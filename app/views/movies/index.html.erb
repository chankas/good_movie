<h2><%= t('.title') %> </h2>
<% @movies.each do |movie| %>
<div>
  <%= image_tag movie.poster_path ? movie.poster_path : 'Image-not-found.png' %>
  <div class="movie">
    <p><%= movie.title %></p>
    <p><%= movie.original_title %></p>
    <p><%= movie.overview %></p>
    <p><%= movie.poster_path %></p>
    <p><%= movie.original_language %></p>
    <p><%= movie.release_date %></p>
  </div>
</div>
<% end %>